{
  "name": "@digitalbazaar/http-signature-zcap-verify",
  "version": "10.1.1-0",
  "description": "A library for invoking Authorization Capabilities via HTTP signatures",
  "license": "BSD-3-Clause",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "cross-env NODE_ENV=test mocha -r esm --preserve-symlinks -t 30000 -A -R ${REPORTER:-spec} --require tests/test-mocha.js tests/*.spec.js",
    "test-karma": "karma start karma.conf.js",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=text-lcov npm run test-node > coverage.lcov",
    "coverage-report": "nyc report",
    "lint": "eslint ."
  },
  "files": [
    "index.js",
    "main.js",
    "util-browser.js",
    "util.js"
  ],
  "dependencies": {
    "@digitalbazaar/http-signature-header": "^4.0.1",
    "@digitalbazaar/zcap": "^7.1.0",
    "base64url-universal": "^1.0.0",
    "esm": "^3.2.22",
    "pako": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@digitalbazaar/ed25519-signature-2020": "^3.0.0",
    "@digitalbazaar/ed25519-verification-key-2020": "^3.1.0",
    "@digitalbazaar/security-context": "^1.0.0",
    "@digitalbazaar/security-document-loader": "^1.0.0",
    "aes-key-wrapping-2019-context": "^1.0.3",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-bytes": "^0.1.2",
    "cross-env": "^7.0.3",
    "crypto-ld": "^6.0.0",
    "eslint": "^8.6.0",
    "eslint-config-digitalbazaar": "^2.6.1",
    "@digitalbazaar/http-signature-zcap-invoke": "^5.0.0",
    "karma": "^6.1.1",
    "karma-babel-preprocessor": "^8.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.4.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "path-to-regexp": "^6.2.0",
    "route-params": "^0.2.0",
    "sha256-hmac-key-2019-context": "^1.0.3",
    "sinon": "^12.0.1",
    "webpack": "^5.44.0",
    "zcap-context": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/http-signature-zcap-verify"
  },
  "keywords": [
    "authorization",
    "capability",
    "authorization capability",
    "object capability",
    "ocap-ld",
    "http signature",
    "http signatures",
    "zcap",
    "zcaps"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/http-signature-zcap-verify/issues"
  },
  "homepage": "https://github.com/digitalbazaar/http-signature-zcap-verify",
  "module": "main.js",
  "browser": {
    "./util.js": "./util-browser.js"
  },
  "engines": {
    "node": ">=12"
  },
  "nyc": {
    "exclude": [
      "tests"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  }
}
